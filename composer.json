{
    "name": "adshares/cms-bundle",
    "description": "CMS bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Adshares",
            "homepage": "https://captainreach.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/adshares/cms-bundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "doctrine/orm": "^2.12",
        "psr/log": "^3.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "6.1.*",
        "symfony/config": "6.1.*",
        "symfony/dependency-injection": "6.1.*",
        "symfony/http-kernel": "6.1.*",
        "symfony/routing": "6.1.*",
        "symfony/security-core": "6.1.*",
        "symfony/security-http": "6.1.*",
        "symfony/service-contracts": "^3.0",
        "symfony/translation": "6.1.*",
        "symfony/validator": "6.1.*",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpcompatibility/php-compatibility": "^9.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/stopwatch": "6.1.*",
        "symfony/var-dumper": "6.1.*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adshares\\CmsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adshares\\CmsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "qa-check": [
            "security-checker security:check",
            "parallel-lint --no-colors --blame --exclude vendor .",
            "phpcs -s src --standard=PHPCompatibility",
            "phpcs -s src --standard=PSR21"
        ]
    }
}
