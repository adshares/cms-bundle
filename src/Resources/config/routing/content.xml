<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="cms_content_history" path="/content/history" methods="GET">
        <default key="_controller">cms.controller.content::history</default>
    </route>

    <route id="cms_content_patch" path="/content" methods="PATCH">
        <default key="_controller">cms.controller.content::patch</default>
    </route>

</routes>
